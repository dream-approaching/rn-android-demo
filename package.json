{
  "name": "MyReactNativeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "dev": "react-native start --reset-cache",
    "debug": "react-native run-android --variant=ceshiDebug",
    "releaseStaging": "react-native run-android --variant=ceshiReleaseStaging",
    "release": "react-native run-android --variant=ceshiRelease",
    "clean": " watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-* && rm -rf node_modules/ && rm -rf $TMPDIR/haste-map-* && npm cache clean --force && yarn",
    "bundleTab3": "rm -rf android/app/src/main/assets/tab3.bundle && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/tab3.bundle"
  },
  "dependencies": {
    "dva-core": "^1.4.0",
    "dva-loading": "^3.0.2",
    "moment": "^2.24.0",
    "react": "16.8.3",
    "react-native": "^0.59.4",
    "react-native-code-push": "^5.6.0",
    "react-native-device-info": "^1.5.0",
    "react-native-gesture-handler": "^1.1.0",
    "react-navigation": "3.8.1",
    "react-redux": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "metro-react-native-babel-preset": "^0.53.1",
    "redux-logger": "^3.0.6"
  }
}
